{
  "name": "netlify-plugin-pastel",
  "version": "0.0.1",
  "description": "Netlify Build plugin - A Netlify build plugin to add in-place commenting (via Pastel) to Netlify deploy previews.",
  "main": "src/main.js",
  "files": [
    "src",
    "!*~"
  ],
  "keywords": [
    "netlify-plugin",
    "netlify"
  ],
  "author": "Jason Lengstorf <jason.lengstorf@netlify.com> <jason.lengstorf@netlify.com>",
  "license": "MIT",
  "repository": "jlengstorf/netlify-plugin-pastel",
  "directories": {
    "lib": "src",
    "test": "test"
  },
  "engines": {
    "node": ">10"
  },
  "scripts": {
    "build": "netlify-build",
    "test": "npm run lint && npm run ava",
    "lint": "npm run eslint && npm run prettier",
    "eslint": "eslint --ignore-path .gitignore --fix --cache --format=codeframe --max-warnings=0 \"{src,init}/**/*.js\"",
    "prettier": "prettier --ignore-path .gitignore --write --loglevel warn \"{.github,src,init}/**/*.{js,md,yml,json}\" \"*.{js,md,yml,json}\"",
    "ava": "ava --verbose",
    "release": "release-it"
  },
  "dependencies": {},
  "devDependencies": {
    "@netlify/build": "^0.1.37",
    "ava": "^2.4.0",
    "eslint": "6.8.0",
    "eslint-config-prettier": "^6.9.0",
    "eslint-plugin-import": "^2.20.0",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-prettier": "^3.1.2",
    "execa": "^4.0.0",
    "netlify-cli": "^2.30.0",
    "prettier": "^1.19.1",
    "release-it": "^12.4.3"
  }
}
